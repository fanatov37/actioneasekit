{
    "name": "fanatov37/actioneasekit",
    "type": "symfony-bundle",
    "description": "ActionEaseKit is a Symfony library designed for rapidly creating actions and providing convenient services for developers",
    "keywords": [
        "actioneasekit",
        "fanatov37",
        "symfony",
        "symfony action ease kit",
        "symfony ease",
        "symfony bundle",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vlad Fanatov",
            "email": "fan.oldschool@gmail.com",
            "homepage": "https://www.linkedin.com/in/fan37/"
        }
    ],

    "require": {
        "php": "^8.1",
        "doctrine/orm": "^2.12",
        "doctrine/annotations": "^2.0",
        "friendsofsymfony/rest-bundle": "^3.1",
        "psr/log": "^2.0",
        "symfony/console": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/process": "6.4.*",
        "symfony/yaml": "6.4.*",
        "symfony/options-resolver": "6.4.*"

    },
    "require-dev": {
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "doctrine/doctrine-bundle": "^2.6",
        "jms/serializer-bundle": "^5.3",
        "symfony/asset": "^6.4",
        "symfony/flex": "^1.0",
        "symfony/monolog-bundle": "^3.7",
        "symfony/property-access": "^6.4",
        "symfony/property-info": "^6.4",
        "symfony/proxy-manager-bridge": "^6.4",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "^6.4",
        "symfony/serializer": "^6.4",
        "symfony/validator": "^6.4",
        "symfony/dotenv": "6.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "ActionEaseKit\\": "src/"
        }
    },

    "replace": {
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "require-all" : true,
    "minimum-stability": "dev"
}
